2.
worker_processes  2;

events {}

http {
    include mime.types;

    server{
    listen 8000;
 
    location /server {
	proxy_set_header proxied nginx;
	proxy_pass http://localhost:5000/;
     }
    
    location /nginxorg {
	proxy_pass https://nginx.org/;
     }

   }
}

3.
http {
    include mime.types;

    server{
    listen 8080;
    location /{
    root /data/www;
	}

    location /images{
    root /data;
	}

    }
}
